      "bbox": [
        103,
        489,
        314,
        506
      ],
      "content": "## [電気主任技術者の解任]",
      "confidence": null,
      "metadata": {
        "line_number": 34,
        "raw_line": "<|ref|>sub_title<|/ref|><|det|>[[103, 489, 314, 506]]<|/det|>"
